{
  "name": "ember-client-gamechanger",
  "version": "1.0.1",
  "description": "Addon to create a front ember application connected to a GraphQL API",
  "keywords": [
    "ember-addon",
    "graphql"
  ],
  "repository": "https://github.com/GameChangerCloud/addon-ember-client-gamechanger",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "mirage": "ember serve --environment=mirage",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "nodetest": "mocha node-tests --recursive"
  },
  "dependencies": {
    "ember-cli-babel": "^7.23.1",
    "ember-cli-htmlbars": "^5.3.2",
    "easygraphql-parser": "0.0.15",
    "ember-concurrency": "2.0.3",
    "ember-bootstrap": "^4.6.3",
    "ember-data": "^3.25.0",
    "ember-cognito": "^0.12.0",
    "graphql": "^15.1.0",
    "inflection": "^1.12.0",
    "pluralize": "^8.0.0",
    "ember-models-table": "^3.4.0",
    "ember-changeset-validations": "^3.14.2",
    "ember-auto-import": "^1.11.2",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-flash": "^2.1.1",
    "ember-cli-mirage": "^2.1.0",
    "ember-faker": "^1.6.0",
    "ember-composable-helpers": "^4.4.1",
    "ember-cli-string-helpers": "^5.0.0",
    "bootstrap": "^4.6.0",
    "ember-cli-string-utils": "^1.1.0",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.2.0",
    "@glimmer/component": "^1.0.3",
    "@glimmer/tracking": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.25.3",
    "ember-cli-app-version": "^4.0.0",
    "ember-cli-blueprint-test-helpers": "^0.18.3",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.3",
    "ember-cli-terser": "^4.0.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.0.4",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.1",
    "ember-qunit": "^5.1.2",
    "ember-resolver": "^8.0.2",
    "ember-simple-auth": "^3.1.0",
    "ember-source": "~3.25.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.18.1",
    "ember-try": "^1.4.0",
    "ember-welcome-page": "^4.0.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-ember": "^10.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "loader.js": "^4.7.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "qunit": "^2.14.0",
    "qunit-dom": "^1.6.0"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
